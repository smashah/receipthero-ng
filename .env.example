# ReceiptHero Environment Variables
# Copy this file to .env.local and fill in your secret values

# === Database ===
DATABASE_PATH=./receipthero.db

# === Config File Path ===
CONFIG_PATH=./config.json

# === Server ===
PORT=3001

# === Paperless-NGX Integration ===
PAPERLESS_HOST=http://localhost:8000
PAPERLESS_API_KEY=

# === AI Processing ===
TOGETHER_API_KEY=

# === Optional: Processing Settings ===
# SCAN_INTERVAL=60000
# RECEIPT_TAG=receipt
# PROCESSED_TAG=processed
# FAILED_TAG=failed
# MAX_RETRIES=3

# === Optional: Rate Limiting (Upstash) ===
# RATE_LIMIT_ENABLED=false
# UPSTASH_URL=
# UPSTASH_TOKEN=

# === Optional: Observability (Helicone) ===
# HELICONE_ENABLED=false
# HELICONE_API_KEY=
